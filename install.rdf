<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>seleniumdotformat@gmail.com</em:id>
		<em:name>Selenium Dot Formatter</em:name>
		<em:version>1.0</em:version>
		<em:creator>Emidio STANI</em:creator>
		<em:description>A Dot Formatter for Selenium IDE</em:description>
		<em:type>2</em:type>
		<em:iconURL>chrome://dotformatter/content/selenium-dot-logo.png</em:iconURL>
   		<em:homepageURL>http://kaosktrl.wordpress.com/2011/12/20/selenium-dot-formatter/</em:homepageURL>

		<!-- Its a firefox plugin -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>1.5</em:minVersion>
				<em:maxVersion>4.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<!-- This is an Se-IDE plugin, so we need to specify it as a requirement -->
		<em:requires>   
			<Description>
				<em:id>{a6fd85ed-e919-4a43-a5af-8da18bda539f}</em:id>
				<em:minVersion>1.0.4-SNAPSHOT</em:minVersion>
				<em:maxVersion>1.*</em:maxVersion>
			</Description>  
		</em:requires>
	</Description>
</RDF>
